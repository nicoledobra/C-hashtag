using System;

class Program
{
    static void Main()
    {
        for (char c = 'A'; c <= 'Z'; c++)
        {
            Console.WriteLine(c);
        }

        for (char c = 'Z'; c >= 'A'; c--)
        {
            Console.WriteLine(c);
        }

        Console.WriteLine("Press any key.");
        Console.ReadKey();
    }
}

